var swtch = 1;
var number = 1;
var primes = 1;
while(swtch == 1){
	document.write("1primes = ", primes);
	if(isPrime(5)){
		primes += 1;
		document.write("primes = ", primes);
	}
	if(primes == 10001){
		document.write("number = ", number);
		swtch = 0;
	}
	number += 2;
}

function isPrime(n){
	var k = int(Math.sqrt(n));
	if(n == 1){
		return false;
	}
	if(n == 2){
		return true;
	}
	for(var i = 2; i <= k; i++){
		if(n % i == 0){
			return false;
		}
	}
	return true;
}